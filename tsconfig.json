{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
		"allowJs": true,
		"jsx": "preserve",
		"baseUrl": "/",
    "paths": {
      "@/*": ["src/*"],
      "@/pages/*": ["src/pages/*"],
      "@/components/*": ["src/components/*"],
			"@/config/*": ["src/config/*"],
    },
		"allowSyntheticDefaultImports": true
	},
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "./node_modules"
  ]
}
