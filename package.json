{
  "name": "react-webpack-mpa-template",
  "version": "0.0.1",
  "description": "react16+webpack4多页应用模板，适用于企业官网等需要SEO的网站。",
  "main": "index.js",
  "scripts": {
    "local": "npm run dev --m local",
    "pro": "npm run build --m pro",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.dev.conf.js",
    "build:uat": "cross-env webpack --config build/webpack.uat.conf.js",
    "build": "cross-env webpack --config build/webpack.prod.conf.js",
    "server": "live-server ./ --port=8888"
  },
  "author": "CellerChan",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^8.5.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.5",
    "css-loader": "^0.28.11",
    "editorconfig": "^0.15.3",
    "exports-loader": "^0.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "glob": "^7.1.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "html-withimg-loader": "^0.1.16",
    "live-server": "^1.2.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^4.0.1",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "postcss-url": "^7.3.2",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "sass-loader": "^7.0.1",
    "script-loader": "^0.7.2",
    "style-loader": "^0.21.0",
    "ts-loader": "^6.2.0",
    "typescript": "^3.6.3",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^2.13.0",
