    "@types/react": "^16.9.5",
    "axios": "^0.19.0",
    "crypto-js": "^3.1.9-1",
    "raw-loader": "^1.0.0",
    "react": "^16.10.0",
    "react-dom": "^16.10.0",
    "webpack": "^4.29.6"
  },
  "browserslist": [
    "defaults",
    "not ie < 11",
    "last 2 versions",
    "> 1%",
    "iOS 7",
    "last 3 iOS versions"
  ]
}
